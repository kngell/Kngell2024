{
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false
  },
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off"
  }
}
